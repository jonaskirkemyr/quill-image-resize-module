{
    "name": "quill-image-resize-module",
    "version": "3.0.2",
    "description": "A module for Quill rich text editor to allow images to be resized.",
    "main": "dist/ImageResize.js",
    "types": "dist/ImageResize.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "release": "npm run lint && npm run build:ts",
        "demo": "npm run release && npm run build:web",
        "lint": "node_modules/.bin/tslint --config tslint.json --project tsconfig.json",
        "fixlint": "npm run lint --fix",
        "build:ts": "tsc -p tsconfig.json",
        "build:web": "node_modules/.bin/webpack --config webpack.config.js -p"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kensnyder/quill-image-resize-module.git"
    },
    "keywords": [
        "quill",
        "quilljs",
        "image",
        "resize",
        "sizer"
    ],
    "author": "@kensnyder",
    "contributors": [
        {
            "name": "Wikia",
            "url": "https://github.com/Wikia"
        },
        {
            "name": "jonaskirkemyr",
            "url": "https://github.com/jonaskirkemyr"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kensnyder/quill-image-resize-module/issues"
    },
    "homepage": "https://github.com/kensnyder/quill-image-resize-module#readme",
    "devDependencies": {
        "@types/lodash": "^4.14.64",
        "@types/node": "^7.0.18",
        "@types/quill": "0.0.29",
        "svg-inline-loader": "^0.7.1",
        "tslint": "^5.3.2",
        "typescript": "^2.3.2",
        "webpack": "^2.3.3"
    },
    "dependencies": {
        "lodash": "^4.17.4",
        "quill": "^1.2.2",
        "ts-loader": "^2.0.3"
    }
}
